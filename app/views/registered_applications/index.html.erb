<h1>Your Registered Applications</h1>

<div class="row">
  <% @registered_applications.each do |registered_application| %>
    <p><%= link_to registered_application.name, registered_application_path(registered_application) %>
  <% end %>
</div>
<div class="row">
  <%= link_to "New Application", new_registered_application_path, class: 'btn btn-success' %>
</div>
